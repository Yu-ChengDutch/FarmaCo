                                                },
                                                {
                                                    "Name": "Hypotropia",
                                                    "Definition": "A downwards deviation of one or two of the eyes"
                                                },
                                                {
                                                    "Name": "Comitant strabismus",
                                                    "Definition": "Strabismus with an equal amount of con- or divergence regardless of visual direction"
                                                },
                                                {
                                                    "Name": "Incomitant strabismus",
                                                    "Definition": "Strabismus with a differing amount of con- or divergence based on visual direction",
                                                    "Subdivision": [
                                                        {
                                                            "Name": "A-pattern strabismus",
                                                            "Definition": "Incomitant strabismus where there's more convergence when looking up and more divergence when looking down"
                                                        },
                                                        {
                                                            "Name": "V-pattern strabismus",
                                                            "Definition": "Incomitant strabismus where there's more divergence when looking up and more convergence when looking down"
                                                        }
                                                    ]
                                                },
                                                {
                                                    "Name": "Manifest strabismus",
                                                    "Definition": "Strabismus which is always present; where the uncovered eye starts to deviate"
