                        {
                            "Name": "Orthopaedic physical examination signs",
                            "Subdivision": [
                                {
                                    "Name": "Galeazzi sign",
                                    "Alternative name": "Allis sign",
                                    "Definition": "Flexing an infants knees when they are laying down. Positive when the knees are not level",
                                    "Causes": ["Hip dysplasia"]
                                },
                                {
                                    "Name": "Patellar grind test",
                                    "Alternative name": "Clarke's test",
                                    "Definition": "Actively contracting the quadriceps while exerting external force distal-wise on the proximal patella. Positive if painful",
                                    "Causes": ["Patellofemoral pain syndrome", "Chonromalcia patella", "Patellofemoral arthritis"]
                                },
                                {
                                    "Name": "Drawer tests",
                                    "Subdivision": [
                                        {
                                            "Name": "Anterior drawer test",
                                            "Definition": "With a knee hanging in flexion, pull the tibia anteriorly. Positive if increased ROM",
                                            "Causes": ["Anterior cruciate ligament injury"]
                                        },
                                        {
                                            "Name": "Posterior drawer test",
                                            "Definition": "With a knee hanging in flexion, pull the tibia posteriorly. Positive if increased ROM",
                                            "Causes": ["Posterior cruciate ligament injury"]
                                        }
                                    ]
                                },
                                {
                                    "Name": "Lachman test",
                                    "Definition": "With the knee flexed at 15 degrees, place one hand on the femur and the other on the tibia. Pull the tibia to anterior. Positive if increased ROM",
                                    "Causes": ["Anterior cruciate ligament injury"]
                                },
                                {
                                    "Name": "Valgus stress test",
                                    "Definition": "With the knee in extension, push the knee into valgus. Positive if the knee opens up medially",
                                    "Causes": ["Medial collateral ligament injury", "Capsular injury of the knee", "Cruciate ligament laxity"]
                                },
                                {
                                    "Name": "Apley grind test",
                                    "Alternative name": "Apley test",
                                    "Definition": "With the patient face down, passively flex the knee to 90 degrees. Compress the tibia into the knee joint with external rotation. Positive if painful",
                                    "Causes": ["Meniscus injury"]
                                },
                                {
                                    "Name": "McMurray test",
                                    "Causes": ["Meniscus injury"]
                                },
                                {
                                    "Name": "Hubscher maneuver",
                                    "Definition": "With a standing patient with a pes planus, take the hallux and dorsiflex. Positive if arch formation.",
                                    "Causes": ["Flexible pes planus"]
                                },
                                {
                                    "Name": "Thompson test",
                                    "Definition": "Have the patient lay prone with the legs hanging of the table. Negative if the feet move slightly in plantarflexion. Positive if foot hangs straight down.",
                                    "Casues": ["Achilles tendon injury"]
                                },
                                {
                                    "Name": "Bear hug test",
                                    "Definition": "Place the ipsilateral hand on the contralateral shoulder. Examiner tries to lift off the hand, patient resits. Positive if resitance is futile.",
                                    "Causes": ["Subscapularis injury"]
                                },
                                {
                                    "Name": "Belly press test",
                                    "Definition": "Have the patient press their belly with the ipsilateral hand. Examiner tries to remove the hand from belly. Positive if not possible.",
                                    "Causes": ["Subscapularis injury"]
                                },
                                {
                                    "Name": "Cozen's test",
                                    "Definition": "Wrist extension against fully extended elbow. Positive if painful",
                                    "Causes": ["Lateral epicondylitis"]
                                },
                                {
                                    "Name": "Elbow extension test",
                                    "Definition": "With the shoulder flexed to 90 degrees, extend the elbow in supine position. Positive if not possible",
                                    "Causes": ["Elbow fracture"]
                                },
                                {
                                    "Name": "Finkelstein's test",
                                    "Definition": "Examiner grasps the thumb and the sharply ulnar deviates the wrist. Positive if sharp pain along the distal radius.",
                                    "Causes": ["De Quervain's tenosynovitis"]
                                },
                                {
                                    "Name": "Schober's test",
                                    "Definition": "Mark 5 cm above L5 and 5 cm below L5. Then ask patient to bend at the hips and touch toes. Positive if distance does not increase >5cm",
                                    "Causes": ["Ankylosing spondylitis"]
                                },
                                {
                                    "Name": "Larrey's sign",
                                    "Definition": "Let the patient sit down fast on a hard chair. Positive if this elicits lower backpain.",
                                    "Causes": ["Sacroiliitis"]
                                }
                            ]
                        }
